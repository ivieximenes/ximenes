		<interface id="addembedded" name="STR_FORM" type="lum_contentAdministrationEmbeddedAddPropertyPage">
			<isAdministration>false</isAdministration>
			<styles>
				<style name="form" path="br/com/qualicorp/redenarede/service/businesscontact/style/form.xsl" id="form" />
				<style name="form-beneficiario" path="br/com/qualicorp/redenarede/service/businesscontact/style/form-beneficiario.xsl" id="form-beneficiario" />
				<style name="form-prestador" path="br/com/qualicorp/redenarede/service/businesscontact/style/form-prestador.xsl" id="form-prestador" />
			</styles>
		</interface>
		
		<!-- administration interfaces -->
		<interface id="administration" name="STR_ADMINISTRATION" type="lum_contentAdministrationList">
			<isAdministration>true</isAdministration>
			
		</interface>
				
		<interface id="configuration" name="STR_CONFIGURATION" type="lum_douiAdministrationAddPropertyPage"/>
