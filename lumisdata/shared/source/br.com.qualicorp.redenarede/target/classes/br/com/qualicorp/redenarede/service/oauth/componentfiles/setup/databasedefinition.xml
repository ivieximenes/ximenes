<databaseDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.lumis.com.br/lumisportal/xsd/databasedefinition.xsd">
	
	<table name="QC_OAUTH">
		<fields>
			<field name="refresh_token" type="string" 		isNotNull="true"/>
			<field name="user_id" 		type="string" 		isNotNull="true" />
			<field name="system" 		type="string" 		isNotNull="true" />
		</fields>
		<constraints>
			<constraint name="QC_OAUTH_PK" type="primaryKey">
				<fields>
					<field name="refresh_token"/>
				</fields>
			</constraint>
		</constraints>
	</table>
	
</databaseDefinition>